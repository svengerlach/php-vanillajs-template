{
    "name": "svengerlach/php-vanillajs-template",
    "description": "Port of John Resig's micro templating approach to PHP for server-side pre-compilation of simple JavaScript templates",
    "type": "library",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.4",
        "twig/twig": "^1.24"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Gerlach",
            "email": "sven.gerlach@qmarketing.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Svengerlach\\VanillaJSTemplate\\": "src/"
        }
    }, 
    "minimum-stability": "stable"
}
